        "data_collection_timeout_seconds": 1.0,
        "save_logs": true,
        "experimental_design": {
            "type": "tps_controlled_comparison",
            "description": "Comprehensive comparison of all strategies under a multi-stage dynamic workload designed to test adaptability, hotspot response, and resilience to cache-unfriendly patterns.",
            "rationale": "Use single-threaded precise TPS control to isolate cache strategy effects from database lock competition. The workload phases are designed to specifically test the hypotheses in EXPERIMENT_PLAN.md.",
            "controlled_variables": [
                "total_cache_size",
                "workload_pattern",
                "data_size",
                "num_databases",
                "tps_per_database"
            ]
        }
    },
